CREATE TABLE customers (
	id INTEGER PRIMARY KEY AUTOINCREMENT,
	full_name TEXT,
	phone number TEXT
	email TEXT,
	street_address TEXT,
	city TEXT,
	state TEXT,
	zip_code TEXT)
	
